{
  "version": "1.0",
  "date": "2026-01-04",
  "description": "Baseline тесты для измерения качества поиска",
  "tests": [
    {
      "category": "exact",
      "description": "Точные запросы (артикул/бренд/размер)",
      "queries": [
        {
          "id": "exact_001",
          "query": "труба стальная 100х100",
          "type": "material",
          "expected_keywords": ["труба", "100", "стальная"]
        },
        {
          "id": "exact_002",
          "query": "цемент м500",
          "type": "material",
          "expected_keywords": ["цемент", "м500", "500"]
        },
        {
          "id": "exact_003",
          "query": "арматура 12 мм",
          "type": "material",
          "expected_keywords": ["арматура", "12"]
        },
        {
          "id": "exact_004",
          "query": "кирпич керамический 250",
          "type": "material",
          "expected_keywords": ["кирпич", "керамический", "250"]
        },
        {
          "id": "exact_005",
          "query": "профиль 60х27",
          "type": "material",
          "expected_keywords": ["профиль", "60", "27"]
        },
        {
          "id": "exact_006",
          "query": "гипсокартон КНАУФ 12.5",
          "type": "material",
          "expected_keywords": ["гипсокартон", "кнауф", "12"]
        },
        {
          "id": "exact_007",
          "query": "утеплитель роквул 50",
          "type": "material",
          "expected_keywords": ["утеплитель", "роквул", "50"]
        },
        {
          "id": "exact_008",
          "query": "металлочерепица монтеррей",
          "type": "material",
          "expected_keywords": ["металлочерепица", "монтеррей"]
        },
        {
          "id": "exact_009",
          "query": "краска супербелая 10л",
          "type": "material",
          "expected_keywords": ["краска", "супербелая", "10"]
        },
        {
          "id": "exact_010",
          "query": "доска 150х50х6000",
          "type": "material",
          "expected_keywords": ["доска", "150", "50", "6000"]
        }
      ]
    },
